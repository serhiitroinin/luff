{
  "name": "life-cli",
  "version": "0.1.0",
  "module": "src/cli.ts",
  "type": "module",
  "private": true,
  "bin": {
    "life": "src/cli.ts"
  },
  "scripts": {
    "dev": "bun run src/cli.ts",
    "build": "bun build src/cli.ts --compile --outfile life",
    "build:all": "bun build src/cli.ts --compile --outfile life --target=bun-darwin-arm64 && bun build src/cli.ts --compile --outfile life-x64 --target=bun-darwin-x64"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.3"
  }
}
